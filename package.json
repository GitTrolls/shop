{
  "name": "@polymer/shop",
  "flat": true,
  "version": "3.0.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "scripts": {
    "serve": "polymer serve",
    "build:prpl-server": "polymer build && rm -rf server/build && mv build server/ && LC_ALL=C find server/build -type f -exec sed -i '' s/node_modules/node_assets/g {} + && mv server/build/es5-bundled/node_modules server/build/es5-bundled/node_assets && mv server/build/es6-bundled/node_modules server/build/es6-bundled/node_assets && mv server/build/esm-bundled/node_modules server/build/esm-bundled/node_assets",
    "build:static": "polymer build --preset es6-bundled --js-transform-modules-to-amd",
    "build": "npm run build:prpl-server && npm run build:static",
    "deploy": "gcloud app deploy server/app.yaml --project polymer-shop --version 3-prpl-server --no-promote"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.12",
    "@polymer/app-route": "^3.0.0-pre.12",
    "@polymer/app-storage": "^3.0.0-pre.12",
    "@polymer/iron-flex-layout": "^3.0.0-pre.12",
    "@polymer/iron-form": "^3.0.0-pre.12",
    "@polymer/iron-icon": "^3.0.0-pre.12",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.12",
    "@polymer/iron-media-query": "^3.0.0-pre.12",
    "@polymer/iron-overlay-behavior": "^3.0.0-pre.12",
    "@polymer/iron-pages": "^3.0.0-pre.12",
    "@polymer/iron-selector": "^3.0.0-pre.12",
    "@polymer/paper-behaviors": "^3.0.0-pre.12",
    "@polymer/paper-icon-button": "^3.0.0-pre.12",
    "@polymer/paper-spinner": "^3.0.0-pre.12",
    "@polymer/polymer": "^3.0.0-pre.12",
    "polymer-cli": "^1.7.0-pre.10"
  },
  "devDependencies": {}
}
