{
  "name": "@polymer/shop",
  "flat": true,
  "version": "3.0.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/shop",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.12",
    "@polymer/app-route": "^3.0.0-pre.12",
    "@polymer/app-storage": "^3.0.0-pre.12",
    "@polymer/iron-flex-layout": "^3.0.0-pre.12",
    "@polymer/iron-form": "^3.0.0-pre.12",
    "@polymer/iron-icon": "^3.0.0-pre.12",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.12",
    "@polymer/iron-media-query": "^3.0.0-pre.12",
    "@polymer/iron-overlay-behavior": "^3.0.0-pre.12",
    "@polymer/iron-pages": "^3.0.0-pre.12",
    "@polymer/iron-selector": "^3.0.0-pre.12",
    "@polymer/paper-behaviors": "^3.0.0-pre.12",
    "@polymer/paper-icon-button": "^3.0.0-pre.12",
    "@polymer/paper-spinner": "^3.0.0-pre.12",
    "@polymer/polymer": "^3.0.0-pre.12",
    "@webcomponents/shadycss": "^1.2.0-0",
    "@webcomponents/webcomponentsjs": "^2.0.0-0"
  },
  "devDependencies": {
    "polymer-cli": "^1.7.0-pre.15",
    "prpl-server": "^1.1.0",
    "renamer": "^0.6.1",
    "replace": "^0.3.0"
  }
}
